{
  "name": "FF Dist",
  "description": "Computes a score distribution to determine a likely winner in fantasy football.",
  "version": "0.0.1",
  "permissions": [],
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://fantasy.espn.com/football/boxscore*"],
      "js": ["content_script.js", "typescriptServices.js"]
    }
  ],
  "web_accessible_resources": ["scripts/*"],
  "browser_action": {
    "default_icon": "favicon.png"
  },
  "manifest_version": 2
}
